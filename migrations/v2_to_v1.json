{
    "from": "2.0",
    "to": "1.0",
    "steps": [
        {
            "op": "move",
            "from": "router/security/enabled",
            "to": "router/securityEnabled"
        },
        {
            "op": "unwrap",
            "path": "router/security/roles",
            "unwrapTo": "router/security"
        },
        {
            "op": "mapArray",
            "path": "router/security/*/permissions",
            "rule": {
                "objectToString": true,
                "keyOnly": true
            }
        }
    ]
}